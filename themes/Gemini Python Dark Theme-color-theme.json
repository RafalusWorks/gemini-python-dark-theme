{
	"name": "Gemini Python Dark Theme",
	"colors": {
		"editorBracketHighlight.foreground1": "#C3C3C3",
		"editorBracketHighlight.foreground2": "#C3C3C3",
		"editorBracketHighlight.foreground3": "#C3C3C3",
		"editorBracketHighlight.foreground4": "#C3C3C3",
		"editorBracketHighlight.foreground5": "#C3C3C3",
		"editorBracketHighlight.foreground6": "#C3C3C3",
		"editorCursor.foreground": "#BF86F2",
		// Title bar colors
		"titleBar.activeBackground": "#151515",
		"titleBar.inactiveBackground": "#151515",
		"titleBar.activeForeground": "#FFFFFF",
		"titleBar.inactiveForeground": "#888888",
		"titleBar.border": "#2A2A2A",
		// Tab colors
		"editorGroupHeader.tabsBackground": "#151515",
		"tab.activeBackground": "#1F1F1F",
		"tab.inactiveBackground": "#151515",
		"tab.border": "#2A2A2A",
		// Main editor background
		"editor.background": "#1F1F1F",
		"editorGutter.background": "#1F1F1F",
		// Sidebar and activity bar colors
		"activityBar.background": "#151515",
		"sideBar.background": "#151515",
		"activityBar.border": "#2A2A2A",
		"sideBar.border": "#2A2A2A",
		"sideBarSectionHeader.background": "#151515",
		// Panel and status bar colors
		"panel.background": "#151515",
		"panel.border": "#2A2A2A",
		"statusBar.background": "#151515",
		"statusBar.border": "#2A2A2A"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				// "fontStyle": "italic",
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#81C995"
			}
		},
		{
			"name": "Python Docstrings",
			"scope": [
				"string.quoted.docstring.multi.python",
				"string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
				"string.quoted.docstring.multi.python punctuation.definition.string.end.python"
			],
			"settings": {
				"foreground": "#A67561"
			}
		},
		{
			"name": "Numbers & Constants",
			"scope": [
				"constant.numeric",
				"constant.language"
			],
			"settings": {
				"foreground": "#E78A43"
			}
		},
		{
			"name": "Keywords, Storage & Logic",
			"scope": [
				"keyword.control",
				"storage.type.function.python",
				"storage.type.class.python",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#BF86F2"
			}
		},
		{
			"name": "Functions & Classes",
			"scope": [
				"entity.name.function.python",
				"support.function",
				"meta.class.python",
				"entity.name.type.class.python",
				"punctuation.definition.inheritance.begin.python",
				"punctuation.definition.inheritance.end.python",
			],
			"settings": {
				"foreground": "#F7D161"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator" // Operators example: + - * / = %
			],
			"settings": {
				"foreground": "#C3C3C3"
			}
		},
		{
			"name": "Punctuation & Delimiters",
			"scope": [
				"punctuation",
				"meta.brace",
				"punctuation.definition.list.begin.python",
				"punctuation.definition.list.end.python",
				"punctuation.definition.dict.begin.python",
				"punctuation.definition.dict.end.python",
				"punctuation.separator.python",
				"punctuation.terminator.statement.python"
			],
			"settings": {
				"foreground": "#C3C3C3"
			}
		},
		{
			"name": "Special Variables (Self)",
			"scope": [
				"variable.language.special.self.python"
			],
			"settings": {
				"foreground": "#C3C3C3"
			}
		},
		{
			"name": "Built-in Functions & Types",
			"scope": [
				"support.type",
				"support.type.exception.python",
				"support.function.builtin.python",
			],
			"settings": {
				"foreground": "#C3C3C3"
			}
		},
		{
			"name": "Support Types",
			"scope": [
				"support.type.python",
				"support.type.builtin.python"
			],
			"settings": {
				"foreground": "#699DF0"
			}
		},
		{
			"name": "F-Strings",
			"scope": [
				"meta.fstring.python"
			],
			"settings": {
				"foreground": "#C3C3C3"
			}
		},
		{
			"name": "JSON Property Names",
			"scope": [
				"support.type.property-name.json.comments",
				"punctuation.support.type.property-name.begin.json.comments",
				"punctuation.support.type.property-name.end.json.comments",
				"support.type.property-name.json",
				"punctuation.support.type.property-name.begin.json",
				"punctuation.support.type.property-name.end.json"
			],
			"settings": {
				"foreground": "#FA903E"
			}
		},
		{
			"name": "JSON String Values",
			"scope": [
				"string.quoted.single.json.comments",
				"string.quoted.double.json.comments",
				"punctuation.definition.string.begin.json.comments",
				"punctuation.definition.string.end.json.comments"
			],
			"settings": {
				"foreground": "#81C995"
			}
		}
	]
}